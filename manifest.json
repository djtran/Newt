{
	"name":"New Tab Terminal",
	"description":"Managing Google Chrome with shell commands",
	"version":"0.1",
	"icons":{
		"16":"graphics/icon16.png",
		"48":"graphics/icon48.png",
		"128":"graphics/icon128.png"
	},
	"permissions": [
		"tabs",			//_exit_ should close the terminal tab
		"bookmarks",	//_ls_ and _cd_ can explore /bookmarks and /user
		"history",		//history can be shown as a file or a sep command
		"identity",		//CLI prompt c:
		"browsingData",	//history\browsingData can be cleared
		"processes",	//_ps_ should show chrome processes
		"storage"		//_ls_ and _cd_ can explore /user and /bookmarks
	],
	"chrome_url_overrides":{
		"newtab":"terminal.html"
	},
	"manifest_version":2
}